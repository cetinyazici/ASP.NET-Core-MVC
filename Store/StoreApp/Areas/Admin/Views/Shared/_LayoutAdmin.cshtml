<!DOCTYPE html>
<html lang="tr">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Btk Akademi - @ViewData["Title"]</title>
    <link asp-append-version="true" href="~/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link asp-append-version="true" href="~/css/site.css" rel="stylesheet" />
    <link asp-append-version="true" href="~/lib/font-awesome/css/all.css" rel="stylesheet" />
</head>
<body>
    <header>
        <partial name="_Navbar">
    </header>
    <div class="container">
        <div>
            <partial name="_Notification"></partial>
        </div>
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>



    <partial name="_Footer">

        <script src="~/lib/font-awesome/js/all.min.js"></script>
        <script src="~/js/site.js"></script>
        <script src="~/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
        <script src="~/lib/jquery/jquery.min.js"></script>
        @await RenderSectionAsync("Script", required: false)
</body>
</html>